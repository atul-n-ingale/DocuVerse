{
  "name": "docuverse-frontend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.0",
    "lucide-react": "^0.294.0",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hot-toast": "^2.4.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@testing-library/user-event": "^14.6.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest-axe": "^8.0.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^27.5.1",
    "msw": "^1.3.2",
    "prettier": "^3.0.0",
    "whatwg-fetch": "^3.6.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --ci --watchAll=false",
    "test:debug": "jest --debug",
    "test:update-snapshots": "jest --updateSnapshot",
    "test:unit": "jest --selectProjects='Unit Tests'",
    "test:unit:watch": "jest --selectProjects='Unit Tests' --watch",
    "test:unit:coverage": "jest --selectProjects='Unit Tests' --coverage",
    "test:integration": "jest --selectProjects='Integration Tests'",
    "test:integration:watch": "jest --selectProjects='Integration Tests' --watch",
    "test:integration:coverage": "jest --selectProjects='Integration Tests' --coverage",
    "test:e2e": "jest --selectProjects='E2E Tests'",
    "test:e2e:watch": "jest --selectProjects='E2E Tests' --watch",
    "test:e2e:coverage": "jest --selectProjects='E2E Tests' --coverage",
    "test:parallel": "jest --maxWorkers=4",
    "test:serial": "jest --maxWorkers=1",
    "test:specific": "jest --testNamePattern",
    "test:file": "jest --testPathPattern",
    "test:failed": "jest --onlyFailures",
    "test:changed": "jest --onlyChanged",
    "test:related": "jest --findRelatedTests",
    "test:clean": "jest --clearCache",
    "test:verbose": "jest --verbose",
    "test:silent": "jest --silent",
    "test:bail": "jest --bail",
    "test:no-cache": "jest --no-cache",
    "test:all": "jest --selectProjects='Unit Tests' 'Integration Tests' 'E2E Tests'",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8000"
}
